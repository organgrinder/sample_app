<div class="span4 offset1">

<table class="outer">
	<tr>
		<% 9.times do |i| %>
			<td>
				<table class="inner">
					<tr>
						<% j=0 %>
						<% @sudoku.cube_objects_by_number(i).each do |item| %>
							<td>
								<% if (@locations_updated.include?(item[:location])) %>
									<div class='newly-solved'><b>
								<% end %>
								<%= item[:value] %>
							</td>
							<% if (j==2 || j==5)%>
								</tr><tr>
							<% end %>
							<% j+=1 %>
						<% end %>
					</tr>
				</table>
			</td>
			<% if (i==2 || i==5)%>
				</tr><tr>
			<% end %>
		<% end %>
	</tr>
</table>

</div>
